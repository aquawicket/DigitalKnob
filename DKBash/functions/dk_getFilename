[ -z "$DKINIT" ] && . ./DK


##################################################################################
# dk_getFilename(<path> <output>)
#
#
dk_getFilename () {
	dk_debugFunc
	[ $# -ne 2 ] && dk_error "Incorrect number of parameters"
	
	eval "$2=$(basename "$1")"
}