echo "$0 $*"
#[ -n "$include_dk_test" ] && exit || export readonly include_dk_test=1
[ -z "$DKINIT" ] && . ./DK #$0


##################################################################################
# dk_require()
#
#
dk_require() {
	dk_debugFunc
	[ -z "$1" ] && dk_error "dk_require(<func_name> <$n>) requires 2 parameters. Example dk_require my_func \$1"
	[ -z "$2" ] && dk_error "$1() requires at least 1 parameter"
}