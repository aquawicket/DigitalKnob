echo "$0 $*"
#[ -n "$include_dk_test" ] && exit || export readonly include_dk_test=1
[ -z "$DKINIT" ] && . ./DK.sh #$0


##################################################################################
# dk_fileContains(file, string)
#
#
dk_fileContains() {
	dk_debugFunc
	[ -z "$2" ] && dk_error "file_contains <file.ext> <string> requires 2 parameters"
		
	grep -q "$2" "$1"
}