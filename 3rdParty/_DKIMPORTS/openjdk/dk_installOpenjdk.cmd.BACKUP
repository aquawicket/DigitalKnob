@echo off
call DK

::####################################################################
::# dk_installOpenjdk()
::#
::#
:dk_installOpenjdk () {
	call dk_debugFunc
	if %__ARGC__% NEQ 0 (call dk_error "%__FUNCTION__%(%__ARGC__%): incorrect number of arguments")
	
    call dk_cmakeEval "dk_load('%DKIMPORTS_DIR%/openjdk/DKMAKE.cmake')" "OPENJDK"
	call dk_printVar OPENJDK
    call dk_checkError
goto:eof