<script language="VBScript">
	On Error Resume Next

	strConn = "Driver={Microsoft Access Driver (*.mdb, *.accdb)};Dbq=E:\Database1.accdb;Uid=Admin;Pwd=;"

	Set objConnection = CreateObject("ADODB.Connection")

	MsgBox "Conn: " & Err.Description 
	objConnection.Open strConn
	MsgBox "OpenConn: " & Err.Description
		
	Set objRecordSet = objConnection.Execute("SELECT * FROM User") 

	Do While Not(objRecordSet.EOF)
	  	UserName = objRecordSet.Fields.Item("UserName").Value
		Password = objRecordSet.Fields.Item("Password").Value
		Description = objRecordSet.Fields.Item("Description").Value
		MsgBox "UserName: " & UserName & "Password: " & Password & "Description: " & Description
	  	objRecordSet.MoveNext
	Loop


	objRecordSet.Close
	objConnection.Close
	Set objRecordSet=Nothing
	Set objConnection=Nothing

</script>