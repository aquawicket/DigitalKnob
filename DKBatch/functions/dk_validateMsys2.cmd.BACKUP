@echo off
call DK

::####################################################################
::# dk_validateMsys2()
::#
::#
:dk_validateMsys2 () {
	call dk_debugFunc
	if %__ARGC__% NEQ 0 (call dk_error "%__FUNCTION__%(): incorrect number of arguments")
	
    call dk_cmakeEval "dk_load('%DKIMPORTS_DIR%/msys2/DKMAKE.cmake')" "MSYS2;MSYS2_GENERATOR"
	call dk_printVar MSYS2
	call dk_printVar MSYS2_GENERATOR
    call dk_checkError
goto:eof