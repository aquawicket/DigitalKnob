@echo off
call DK

::####################################################################
::# dk_validateOpenjdk()
::#
::#
:dk_validateOpenjdk () {
	call dk_debugFunc
	if %__ARGC__% NEQ 0 (dk_error "%__FUNCTION__%(): incorrect number of arguments")
	
    call dk_cmakeEval "dk_load('%DKIMPORTS_DIR%/openjdk/DKMAKE.cmake')" "OPENJDK"
	call dk_printVar OPENJDK
    call dk_checkError
goto:eof