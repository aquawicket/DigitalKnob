@echo off
call DK

::####################################################################
::# dk_validateGcc()
::#
::#
:dk_validateGcc () {
	call dk_debugFunc
	if %__ARGC__% NEQ 0 (dk_error "%__FUNCTION__%(): incorrect number of arguments")
	
	if "%DKIMPORTS_DIR%"==""  call dk_validateBranch
    call dk_cmakeEval "dk_load('%DKIMPORTS_DIR%/gcc/DKMAKE.cmake')" "GCC_C_COMPILER;GCC_CXX_COMPILER"
	call dk_printVar GCC_C_COMPILER
	call dk_printVar GCC_CXX_COMPILER
    call dk_checkError
goto:eof